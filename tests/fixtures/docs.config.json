{
	"$schema": "https://raw.githubusercontent.com/fbosch/docs-cache/main/docs.config.schema.json",
	"sources": [
		{
			"id": "vitest",
			"repo": "https://github.com/vitest-dev/vitest.git",
			"include": ["docs/**", "README.md"],
			"exclude": ["docs/**/.cache/**"],
			"integrity": {
				"type": "commit",
				"value": null
			}
		},
		{
			"id": "node",
			"repo": "https://github.com/nodejs/node.git",
			"ref": "v20.11.1",
			"mode": "sparse",
			"depth": 1,
			"include": ["doc/api/**"],
			"required": false,
			"maxBytes": 50000000
		}
	]
}
